<plugins>
	<!-- standard widgets -->
	<plugin module="blurdev.gui.widgets.accordianwidget" 		class="AccordianWidget" container="True"/>
	<plugin module="blurdev.gui.widgets.filepickerwidget" 		class="FilePickerWidget"/>
	<plugin module="blurdev.ide.documenteditor" 				class="DocumentEditor"/>
	<plugin module="blurdev.gui.widgets.colorwidgets" 			class="ColorPickerWidget"/>
	<plugin module="blurdev.gui.widgets.colorwidgets" 			class="ColorPickerButton"/>
	<plugin module="blurdev.gui.widgets.previewwidget"			class="PreviewWidget"/>
	<plugin module="blurdev.gui.widgets.blurtreewidget"			class="BlurTreeWidget"/>
	<plugin module="blurdev.gui.widgets.multiplechoicebutton"	class="MultipleChoiceButton"/>
	
	<!-- include user defined blur prefs widgets -->
	<include href="c:/blur/prefs/designer_plugins.xml"/>
	
	<!-- include trax widgets -->
	<include href="[TRAXDEVPATH]/trax/definitions/designer_plugins.xml" root="[TRAXDEVPATH]"/>
	
	<!-- include blur library plugins -->
	<include href="[BLURLIBSPATH]/designer_plugins.xml"	root="[BLURLIBSPATH]"/>
</plugins>